    {
        "id": "8b8607268cbeda4f",
        "type": "function",
        "z": "2ec9db83d3699390",
        "name": "function 6",
        "func": "// Ambil data lama dari flow\nlet data = flow.get(\"sensor_data\") || {};\n\n// Isi sesuai topic\nif (msg.topic === \"home/brightness\") {\n    data.ldr = msg.payload;\n}\nelse if (msg.topic === \"home/pir\") {\n    data.pir = msg.payload;\n}\n// Simpan kembali\nflow.set(\"sensor_data\", data);\n\n// Keluarkan hanya jika PIR (trigger)\nif (msg.topic !== \"home/pir\") {\n    return null;\n}\n\ndata.waktu = new Date().toLocaleString(\"id-ID\", {\n    timeZone: \"Asia/Jakarta\"\n});\n\n// Output lengkap\nmsg.payload = data;\nreturn msg;\n",
        "outputs": 1,
        "timeout": 0,
        "noerr": 0,
        "initialize": "",
